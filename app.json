{
  "expo": {
    "name": "BSM",
    "slug": "bsm-mobile-client",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bsm.mobile.expo",
      "reversedClientId": "not gonna happen"
    },
    "android": {
      "package": "com.bsm.mobile.expo",
      "googleServicesFile": "./secrets/google-services.json"
    }
  }
}
